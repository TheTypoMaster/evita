[Constructor(optional StyleSheet parentStyleSheet)]
interface StyleSheet {
  readonly StyleSheet? parentStyleSheet;
  void AddRule(DOMString selector, Style style);
  Style? QueryRule(DOMString selector);
  void RemoveRule(DOMString selector);
};
